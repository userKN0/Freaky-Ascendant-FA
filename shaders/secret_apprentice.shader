models\players\secret_apprentice_HK\fular
{
	cull	twosided
    {
        map models\players\secret_apprentice_HK\fular
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
}

models\players\secret_apprentice_HK\torso
{
	cull	twosided
    {
        map models\players\secret_apprentice_HK\torso
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models\players\secret_apprentice_HK\torso_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

